{
  "name": "bovicontrol-backend",
  "version": "1.0.0",
  "description": "Backend API para BoviControl - Sistema de Gestão Pecuária",
  "main": "dist/server.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc -p tsconfig.build.json",
    "build:strict": "tsc",
    "start": "node dist/server.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:studio": "prisma studio",
    "prisma:seed": "tsx src/database/seeds/seed.ts",
    "prisma:seed:master": "tsx src/database/seeds/seed-master.ts",
    "db:setup": "./scripts/setup-database.sh",
    "db:reset": "prisma migrate reset -f && prisma db push && prisma db seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit -p tsconfig.strict.json",
    "build:check": "npm run typecheck && npm run build",
    "fix:types": "tsx scripts/fix-typescript-errors.ts"
  },
  "keywords": [
    "cattle",
    "management",
    "api",
    "nodejs"
  ],
  "author": "BoviControl",
  "license": "ISC",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@bull-board/express": "^6.12.7",
    "@prisma/client": "^5.8.1",
    "@types/cookie-parser": "^1.4.9",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "bcryptjs": "^2.4.3",
    "bull": "^4.16.5",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "date-fns": "^3.3.1",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.7.0",
    "joi": "^17.12.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^2.7.0",
    "pg": "^8.16.3",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node": "^10.9.2",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "axios": "^1.12.1",
    "concurrently": "^9.2.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prisma": "^5.8.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
