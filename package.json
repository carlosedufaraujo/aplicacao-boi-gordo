{
  "name": "bovicontrol",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "node auto-sync-system.cjs",
    "start:sync": "node auto-sync-system.cjs",
    "sync": "node auto-sync-system.cjs",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "dev:full": "npx concurrently \"npm run dev:backend\" \"npm run dev\" \"npm run dev:db\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:db": "cd backend && npx prisma studio --port 5557",
    "install:all": "npm install && cd backend && npm install",
    "db:migrate": "cd backend && npx prisma migrate dev",
    "db:push": "cd backend && npx prisma db push",
    "sync-config": "node scripts/sync-config.cjs",
    "predev": "npm run sync-config",
    "predev:full": "npm run sync-config",
    "db:reset": "cd backend && npx prisma migrate reset",
    "test:e2e": "node tests/e2e/run-tests.cjs",
    "test:e2e:watch": "HEADLESS=false node tests/e2e/run-tests.cjs",
    "test:api": "node tests/api/api-tests.cjs",
    "test:all": "npm run test:run && npm run test:api && npm run test:e2e"
  },
